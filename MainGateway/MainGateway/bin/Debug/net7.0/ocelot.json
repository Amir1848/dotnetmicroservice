{
  "Routes": [
    {
      "DownstreamPathTemplate": "/student/get?id={id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5238
        }
      ],
      "UpstreamPathTemplate": "/api/student/get/{id}",
      "UpstreamHttpMethod": [ "Get" ],
      "SwaggerKey": "general"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "general",
      "Config": [
        {
          "Name": "FUM General API",
          "Version": "v1",
          "Url": "http://localhost:5238/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5000"
  }
}